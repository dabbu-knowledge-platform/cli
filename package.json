{
  "name": "dabbu-cli",
  "version": "2.3.0-alpha8",
  "description": "A CLI that leverages the Dabbu API and neatly retrieves your files and folders scattered online",
  "main": "src/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node src/index.js",
    "clean": "rm -r dist/ && mkdir dist/",
    "check-format": "npx prettier --check .",
    "format": "npx prettier --write .",
    "prebuild": "npm run check-format",
    "build": "pkg --output dist/dabbu-cli .",
    "test": "echo \"Error: no test specified\"; exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gamemaker1/dabbu-cli.git"
  },
  "author": "gamemaker1",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/gamemaker1/dabbu-cli/issues"
  },
  "homepage": "https://github.com/gamemaker1/dabbu-cli#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "cli-table3": "^0.6.0",
    "conf": "^9.0.2",
    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "form-data": "^3.0.0",
    "fs-extra": "^9.1.0",
    "nanoid": "^3.1.20",
    "open": "^7.4.0",
    "ora": "^5.3.0",
    "readcommand": "^0.3.0",
    "terminal-link": "^2.1.1"
  },
  "devDependencies": {
    "pkg": "^4.4.9",
    "prettier": "2.2.1"
  },
  "bin": "src/index.js",
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "src/**/*"
    ],
    "targets": [
      "node14-linux-x64",
      "node14-macos-x64",
      "node14-win-x64"
    ]
  }
}
